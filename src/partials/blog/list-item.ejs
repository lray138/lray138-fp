<%
	href = typeof href != "undefined" ? href : "#";
	hero_src = typeof hero_src != "undefined" ? hero_src : "#";
	hero_alt = typeof hero_alt != "undefined" ? hero_alt : "";
	title = typeof title != "undefined" ? title : "";
	date = typeof date != "undefined" ? date : "";
%>
<li>
	<a href="<%= href %>">
		<img width="720" height="360" src="<%= hero_src %>" alt=""/>
		<h4 class="title"><%- title %></h4>
		<p class="date"><%- date %></p>
	</a>
</li>